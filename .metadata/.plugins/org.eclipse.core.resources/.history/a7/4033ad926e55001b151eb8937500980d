package com.cos.start.web;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

//@RestController

@Controller
public class IndexController {
	
	// http://localhost:8080/start/index
	@GetMapping("/index")
	public String index() {
		return "index 함수입니다";
	}
	
	@GetMapping("/hello")
	public String hello() {
		return "hello!!";
	}
}
